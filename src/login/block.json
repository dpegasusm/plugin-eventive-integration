{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "eventive/login",
	"version": "0.1.0",
	"title": "Eventive Login",
	"category": "eventive",
	"description": "Display Eventive login/logout functionality with modal authentication.",
	"textdomain": "eventive",
	"attributes": {
		"loginLinkText": {
			"type": "string",
			"default": "Log in to your account"
		}
	},
	"supports": {
		"html": false,
		"align": true
	},
	"viewScript": "file:./view.js",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
