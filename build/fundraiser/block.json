{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eventive/fundraiser",
  "version": "0.1.0",
  "title": "Eventive Fundraiser",
  "category": "eventive",
  "description": "Display fundraiser progress with donations from Eventive transactions.",
  "textdomain": "eventive",
  "attributes": {
    "startTime": {
      "type": "string",
      "default": ""
    },
    "endTime": {
      "type": "string",
      "default": ""
    },
    "goalAmount": {
      "type": "number",
      "default": 1000
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "viewScript": "file:./view.js",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}