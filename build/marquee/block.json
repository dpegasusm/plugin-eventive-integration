{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "eventive/marquee",
  "version": "0.1.0",
  "title": "Eventive Marquee",
  "category": "eventive",
  "description": "Display a scrolling marquee of film posters with overlay and caption options.",
  "textdomain": "eventive",
  "attributes": {
    "tag": {
      "type": "string",
      "default": ""
    },
    "exclude": {
      "type": "string",
      "default": ""
    },
    "number": {
      "type": "number",
      "default": 5
    },
    "stills": {
      "type": "boolean",
      "default": false
    },
    "yearRound": {
      "type": "boolean",
      "default": false
    },
    "overlay": {
      "type": "string",
      "default": ""
    },
    "overlayOpacity": {
      "type": "string",
      "default": "0.22"
    },
    "caption": {
      "type": "string",
      "default": ""
    },
    "captionSpeed": {
      "type": "string",
      "default": "match"
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "viewScript": "file:./view.js",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}